{
  "name": "@kasanovaio/dapp-sdk",
  "version": "1.0.2",
  "description": "TypeScript SDK for integrating Kasanova as a dApp wallet â€” KasWare-compatible L1 + EIP-1193 L2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kaspa",
    "kasanova",
    "kasware",
    "wallet",
    "dapp",
    "web3",
    "krc20",
    "kns",
    "blockchain"
  ],
  "author": "Kasanova",
  "homepage": "https://github.com/kasanova-io/dapp-sdk#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasanova-io/dapp-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/kasanova-io/dapp-sdk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false
}
